{
    "name": "express-template",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "node dist/index.js",
        "dev": "./node_modules/.bin/nodemon --config nodemon.json",
        "build": "npm run lint && tsc",
        "lint": "tslint -p .",
        "test": "jest --coverage --runInBand"
    },
    "dependencies": {
        "bunyan": "^1.8.12",
        "bunyan-format": "^0.2.1",
        "config": "^3.3.8",
        "crypto": "^1.0.1",
        "express": "~4.20.0",
        "express-list-endpoints": "^4.0.1",
        "express-validator": "^6.1.1",
        "http-errors": "~1.6.3",
        "jdbc": "^0.7.6",
        "jsonwebtoken": "^9.0.0"
    },
    "devDependencies": {
        "@types/bunyan": "^1.8.6",
        "@types/config": "0.0.34",
        "@types/express": "^4.17.0",
        "@types/express-validator": "^3.0.0",
        "@types/jest": "^24.0.16",
        "jest": "^29.6.1",
        "nodemon": "^3.1.3",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.3.0",
        "tslint": "^5.18.0",
        "typescript": "^3.5.3"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "testEnvironment": "node"
    }
}
