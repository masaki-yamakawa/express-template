{
    "name": "express-template",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "node dist/index.js",
        "dev": "./node_modules/.bin/nodemon --config nodemon.json",
        "build": "npm run lint && tsc",
        "lint": "tslint -p .",
        "test": "jest --coverage --runInBand"
    },
    "dependencies": {
        "config": "^3.2.2",
        "express": "~4.16.1",
        "express-list-endpoints": "^4.0.1",
        "http-errors": "~1.6.3",
        "jdbc": "^0.6.4"
    },
    "devDependencies": {
        "@types/config": "0.0.34",
        "@types/express": "^4.17.0",
        "@types/jest": "^24.0.16",
        "jest": "^24.8.0",
        "nodemon": "^1.19.1",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.3.0",
        "tslint": "^5.18.0",
        "typescript": "^3.5.3"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "testEnvironment": "node"
    }
}
